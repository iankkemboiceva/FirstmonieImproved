<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"

  android:gravity="center"
    android:filterTouchesWhenObscured ="true"



    >

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:id="@+id/rlid"
        android:layout_marginTop="65dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@color/colorPrimary"
        android:alpha="0.97"
        android:layout_height="wrap_content">

        <RelativeLayout
            android:id="@+id/nav_header_container"
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:layout_alignParentTop="true"


            >
            <ImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:id="@+id/profimg"
                android:visibility="gone"
                android:background="@drawable/profilepic"/>

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="10dp"
                android:layout_marginTop="10dp"
                android:id="@+id/closeicon"
                android:backgroundTint="@color/white"
                android:background="@drawable/close_circle"/>



            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"



                android:layout_marginTop="10dp"

                android:textColor="@color/white"
android:layout_centerInParent="true"

                android:textSize="19sp"
                android:id="@+id/txuserid"
                android:layout_below="@+id/profimg"
                android:textStyle="normal"
                android:text="Supervisor Menu"


                />



                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textStyle="bold"
                    android:layout_centerHorizontal="true"
android:visibility="gone"
android:layout_below="@+id/txuserid"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="20dp"

                    android:textColor="@color/white"


                    android:textSize="14sp"
                    android:id="@+id/lastlog"


                    android:text=""


                    />

            <TextView
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:background="@drawable/home"

                android:layout_marginRight="30dp"
                android:layout_marginTop="10dp"
                android:textColor="@color/white"
                android:layout_alignParentRight="true"
                android:id="@+id/home"
                android:textSize="14sp"
                android:visibility="gone"

                android:textStyle="bold"
                android:text=""


                />

        </RelativeLayout>


        <LinearLayout
            android:id="@+id/nav_he"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:gravity="center"
            android:layout_weight="60"
            android:layout_below="@+id/nav_header_container"



            >
            <LinearLayout
                android:id="@+id/lyvert"
                android:layout_width="wrap_content"

                android:layout_height="match_parent"
                android:background="@color/colorPrimary"
                android:orientation="vertical"

                >


                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="0dp"
                    android:gravity="center_horizontal"
                    android:layout_gravity="center"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/rlagentcredit"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="5dp"
                        android:padding="5dp"

                        >

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_centerInParent="true"
                            android:layout_height="wrap_content">

                            <ImageView xmlns:app="http://schemas.android.com/apk/res-auto"
                                android:id="@+id/item5"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:visibility="gone"
                                android:layout_marginLeft="10dp"
                                android:backgroundTint="@color/white"
                                android:src="@drawable/agentcredit"

                                />

                            <TextView
                                android:id="@+id/txt_item5"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="40dp"
                                android:layout_toRightOf="@+id/item5"
                                android:layout_centerVertical="true"

                                android:text="Agent Credit"
                                android:textColor="@color/white"
                                android:textSize="15sp"

                                android:textStyle="normal" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rlreports"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"


                        android:layout_below="@+id/rl4"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="5dp"
                        android:padding="5dp">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"

                            android:layout_marginTop="10dp">

                            <ImageView
                                android:id="@+id/item6"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:backgroundTint="@color/white"
                                android:layout_marginLeft="10dp"
                                android:visibility="gone"
                                android:src="@drawable/report" />

                            <TextView
                                android:id="@+id/txt_item6"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"


                                android:layout_marginLeft="40dp"
                                android:layout_toRightOf="@+id/item6"
                                android:layout_centerVertical="true"

                                android:text="Reports"
                                android:textColor="@color/white"
                                android:textSize="16sp"

                                android:textStyle="normal" />
                        </RelativeLayout>
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/rlmyaccount"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"



                        android:layout_below="@+id/rl4"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="5dp"
                        android:padding="5dp">

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"

                            android:layout_marginTop="10dp">

                            <ImageView
                                android:id="@+id/myaccount"
                                android:layout_width="20dp"
                                android:layout_height="20dp"
                                android:visibility="gone"
                                android:backgroundTint="@color/white"
                                android:layout_marginLeft="10dp"
                                android:background="@drawable/myaccounticon" />

                            <TextView
                                android:id="@+id/myaccounttxt"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"


                                android:layout_marginLeft="40dp"
                                android:layout_toRightOf="@+id/myaccount"
                                android:layout_centerVertical="true"

                                android:text="Account Statement"
                                android:textColor="@color/white"
                                android:textSize="16sp"

                                android:textStyle="normal" />
                        </RelativeLayout>
                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/rltyu"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"


                        android:layout_below="@+id/rl4"
                        android:layout_marginLeft="5dp"
                        android:layout_marginTop="40dp"
                        android:layout_marginRight="5dp"
                        android:padding="5dp">
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:id="@+id/rllogout"
                        android:layout_marginTop="10dp"
android:visibility="gone"


                        android:layout_height="wrap_content">
                        <ImageView
                        android:id="@+id/item9"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                            android:layout_marginLeft="10dp"


                        android:background="@drawable/logout" />


                        <TextView
                            android:id="@+id/txt_item12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:textSize="18sp"
                            android:layout_toRightOf="@+id/item9"

                            android:layout_centerVertical="true"


                            android:layout_marginLeft="50dp"

                            android:textStyle="bold"
                            android:text="Log Out"

                            android:textColor="@color/colorPrimary" />
                    </RelativeLayout>

                    </RelativeLayout>

                </LinearLayout>






            </LinearLayout>



        </LinearLayout>
<!--        <LinearLayout-->
<!--            android:layout_width="match_parent"-->
<!--            android:layout_height="60dp"-->
<!--            android:background="@color/colorPrimary"-->
<!--            android:gravity="center"-->
<!--            android:layout_alignParentBottom="true">-->

<!--            <TextView-->
<!--                android:layout_width="wrap_content"-->
<!--                android:textColor="@color/colorPrimary"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:textSize="14sp"-->
<!--                android:text="close"-->
<!--                />-->
<!--        </LinearLayout>-->
    </LinearLayout>



</LinearLayout>